{
  "plugins": [
    "@semantic-release/changelog",
    ["@semantic-release/npm", {
      "npmPublish": false
    }],
    ["@semantic-release/git", {
      "message": "chore(release): ${nextRelease.version} [ci skip]\n\n${nextRelease.notes}"
    }]
  ],
  "repositoryUrl": "git@bitbucket.org:HARCHHI/drone-test.git",
  "tagFormat": "${version}"
}
